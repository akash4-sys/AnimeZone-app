doctype html
head
  meta(charset='utf-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  title AnimeZone
  meta(name='description' content='')
  meta(name='viewport' content='width=device-width, initial-scale=1')
  style 
    include ../public/css/style.css
    include ../public/css/media.css
  link.logo(rel='icon' href='../resources/Logo.png' type='image/x-icon')
  link(href='https://fonts.googleapis.com/icon?family=Material+Icons' rel='stylesheet')
  link(rel='stylesheet' href='https://unpkg.com/flickity@2/dist/flickity.min.css')
.background
  nav.navbar
    // nbcckground class here
    .burger
      .line
      .line
      .line
    ul.nav-list.invisible
      .logo
        img(src='../resources/Logo.png' alt='logo')
      li
        a.hover(href='#Home') Home
      li
        a.hover(href='#') Anime
      li
        a.hover(href='#') Manga
      li
        a.hover(href='#') Manhwa
      li
        a.hover(href=url) #{value}
    .rightnav
      //
        <input type="text" name="search" id="search" placeholder="Search..." autocomplete="on">
        <button class="btn btn-sm hover">Search</button>
      script(async='' src='https://cse.google.com/cse.js?cx=5845f3d513dd296df')
      #search.gcse-search
  section.firstSection
    // background class here to seperate the nav bar
    .box-main
      .firsthalf
        p.moto ALL YOUR FAVOURITE ANIME AT ONE PLACE
        p.motot Get hot links to your favourite otaku sites
        .home-button
          a(href=url).btn.animate.hover #{value}
          a(href="#").btn.animate.hover Read Now
      .secondhalf
        img(src='../resources/Logo.png' alt='TempImage')
.main-body
  button#tothetop.animate-arrow
    i.material-icons arrow_upward
  section.section.spec-section
    .paras
      p.sectiontag.sub-moto
        | Top Manga Hits of all Time
